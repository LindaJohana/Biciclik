<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.biciclik">

    <uses-feature android:name="android.hardware.camera2"
        android:required="true"/>

    <application
        
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Biciclik">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Registro1"/>
        <activity android:name=".RegistroExitoso"/>
        <activity android:name=".RegistroFotos"/>
        <activity android:name=".Registro3"/>
        <activity android:name=".OlvidoContrasenaActivity"/>
        <activity android:name=".DrawerMain" android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".InicioActivity"/>
    </application>

</manifest>